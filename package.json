{
  "name": "farbenfroh.io",
  "contributors": [
    "winston <hey@winston.sh>"
  ],
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:wasm": "cargo watch -i README.md -i .gitignore -i 'pkg/*' -s 'wasm-pack build --no-typescript --target web --profiling'",
    "build": "yarn build:wasm; next build",
    "build:wasm": "wasm-pack build --no-typescript --target web --release",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "prettier"
    ]
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "comlink": "^4.3.1",
    "next": "^13.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@catppuccin/tailwindcss": "^0.1.1",
    "@svg-icons/bootstrap": "^1.86.0",
    "@svg-icons/fa-brands": "^1.86.0",
    "@svg-icons/octicons": "^1.86.0",
    "@svgr/webpack": "^6.5.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.8",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.27.0",
    "eslint-config-next": "^13.0.4",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.2.4",
    "vercel": "^28.5.5"
  }
}
